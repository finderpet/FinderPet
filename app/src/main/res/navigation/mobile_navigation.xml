<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.finder.pet.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_nav_home_to_tabsActivity"
            app:destination="@id/tabsActivity" />
        <action
            android:id="@+id/action_nav_home_to_foundFragment"
            app:destination="@id/foundFragment" />
        <action
            android:id="@+id/action_nav_home_to_lostFragment"
            app:destination="@id/lostFragment" />
        <action
            android:id="@+id/action_nav_home_to_adoptedFragment"
            app:destination="@id/adoptedFragment" />
    </fragment>

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.finder.pet.ui.gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" >
        <action
            android:id="@+id/action_nav_gallery_to_mapsActivity"
            app:destination="@id/mapsActivity" />
    </fragment>

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.finder.pet.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" >
        <action
            android:id="@+id/action_nav_slideshow_to_nav_profile"
            app:destination="@id/nav_profile" />
    </fragment>

    <fragment
        android:id="@+id/nav_tools"
        android:name="com.finder.pet.ui.tools.ToolsFragment"
        android:label="@string/menu_tools"
        tools:layout="@layout/fragment_tools" >
        <action
            android:id="@+id/action_nav_tools_to_servicesFragment"
            app:destination="@id/servicesFragment" />
        <action
            android:id="@+id/action_nav_tools_to_productsFragment"
            app:destination="@id/productsFragment" />
    </fragment>

    <fragment
        android:id="@+id/nav_share"
        android:name="com.finder.pet.ui.share.ShareFragment"
        android:label="@string/menu_share"
        tools:layout="@layout/fragment_share" />

    <fragment
        android:id="@+id/nav_send"
        android:name="com.finder.pet.ui.send.SendFragment"
        android:label="@string/menu_send"
        tools:layout="@layout/fragment_send" />

    <fragment
        android:id="@+id/contenedorFragment"
        android:name="com.finder.pet.Fragments.ContenedorFragment"
        android:label="fragment_contenedor"
        tools:layout="@layout/fragment_contenedor" />
    <fragment
        android:id="@+id/nav_profile"
        android:name="com.finder.pet.Fragments.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/foundFragment"
        android:name="com.finder.pet.Fragments.FoundFragment"
        android:label="Encontrados"
        tools:layout="@layout/fragment_found" />
    <fragment
        android:id="@+id/lostFragment"
        android:name="com.finder.pet.Fragments.LostFragment"
        android:label="Perdidos"
        tools:layout="@layout/fragment_lost" />
    <fragment
        android:id="@+id/adoptedFragment"
        android:name="com.finder.pet.Fragments.AdoptedFragment"
        android:label="Adopciones"
        tools:layout="@layout/fragment_adopted" />
    <activity
        android:id="@+id/tabsActivity"
        android:name="com.finder.pet.TabsActivity"
        android:label="activity_tabs"
        tools:layout="@layout/activity_tabs" />
    <activity
        android:id="@+id/mapsActivity"
        android:name="com.finder.pet.Main.MapsActivity"
        android:label="activity_maps"
        tools:layout="@layout/activity_maps" />
    <fragment
        android:id="@+id/servicesFragment"
        android:name="com.finder.pet.servicesFragment"
        android:label="Servicios"
        tools:layout="@layout/fragment_services" >
        <action
            android:id="@+id/action_servicesFragment_to_listServicesFragment"
            app:destination="@id/listServicesFragment" />
    </fragment>
    <fragment
        android:id="@+id/productsFragment"
        android:name="com.finder.pet.productsFragment"
        android:label="Productos"
        tools:layout="@layout/fragment_products" >
        <action
            android:id="@+id/action_productsFragment_to_listProductsFragment"
            app:destination="@id/listProductsFragment" />
    </fragment>
    <fragment
        android:id="@+id/listServicesFragment"
        android:name="com.finder.pet.listServicesFragment"
        android:label="fragment_list_services"
        tools:layout="@layout/fragment_list_services" >
        <action
            android:id="@+id/action_listServicesFragment_to_detailsServicesFragment"
            app:destination="@id/detailsServicesFragment" />
    </fragment>
    <fragment
        android:id="@+id/listProductsFragment"
        android:name="com.finder.pet.listProductsFragment"
        android:label="fragment_list_products"
        tools:layout="@layout/fragment_list_products" >
        <action
            android:id="@+id/action_listProductsFragment_to_detailsProductsFragment"
            app:destination="@id/detailsProductsFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailsServicesFragment"
        android:name="com.finder.pet.detailsServicesFragment"
        android:label="fragment_details_services"
        tools:layout="@layout/fragment_details_services" />
    <fragment
        android:id="@+id/detailsProductsFragment"
        android:name="com.finder.pet.detailsProductsFragment"
        android:label="fragment_details_products"
        tools:layout="@layout/fragment_details_products" />
    <activity
        android:id="@+id/loginActivity"
        android:name="com.finder.pet.Conexion.LoginActivity"
        android:label="activity_login"
        tools:layout="@layout/activity_login" />
</navigation>